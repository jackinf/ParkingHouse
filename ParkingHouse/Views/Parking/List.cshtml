@using ParkingHouse.DB.Entities
@model IEnumerable<ParkingHouse.DB.Entities.Car>
@{
    ViewBag.Title = "Cars in the parking house";
}

@foreach (var car in Model)
{
    Html.RenderPartial("CarSummary", car);    
}


@section Add{
    @{
        Html.RenderPartial("AddParkingCar", new Car());
    }    
}
@section EndSimulation{
    @{        
        Html.RenderPartial("EndSimulation");
    }    
   
}
@if (TempData["summarySum"] != null)
{
        <div class="Message">@TempData["summaryCars"] cars visited.</div>
        <div class="Message">Total profit @string.Format("{0:c}", TempData["summarySum"])</div>
}
